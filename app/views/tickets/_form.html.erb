<%= render "/shared/error_messages", target: @ticket %>

<%= simple_form_for @ticket do |f| %>
  <%= f.input :title %>
  <%= f.input :description %>
  <%= f.input :status, as: :select, include_blank: false, collection: Ticket::STATUSES %>
  <%= f.association :submitter %>
  <%= f.button :submit %>
<% end %>

