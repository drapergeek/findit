<%= simple_form_for @ip, :html => { :class => 'form-horizontal' } do |f| %>
  <%= f.input :number %>
  <%= f.association :item %>
  <%= f.association :building %>
  <div class="form-actions">
    <%= f.button :submit, :class => 'btn-primary' %>
    <%= link_to t('.cancel', :default => t("helpers.links.cancel")),
                ips_path, :class => 'btn' %>
  </div>
<% end %>
