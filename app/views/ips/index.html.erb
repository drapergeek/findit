<h1>IPs</h1>
<table class="table table-striped">
  <thead>
    <tr>
      <th>Name</th>
    </tr>
  </thead>
  <tbody>
    <% @ips.each do |ip| %>
      <tr>
        <td><%= ip.number %></td>
        <td>
          <%= link_to 'Edit', edit_ip_path(ip), :class => 'btn btn-mini' %>
          <%= destroy_link(ip) %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= link_to 'New', new_ip_path, :class => 'btn btn-primary' %>
