<div class="block">
  <div class="secondary-navigation">
    <ul class="wat-cf">
      <li class="first active"><%= link_to "#{t("web-app-theme.list", :default => "List")}", dns_names_path  %></li>
      <li><%= link_to "#{t("web-app-theme.new", :default => "New")}", new_dns_name_path %></li>
    </ul>
  </div>          
  <div class="content">          
    <h2 class="title"><%= t("web-app-theme.all", :default => "All")  %> DnsNames</h2>
    <div class="inner">
      <table class="table">
        <tr>             
          <th class="first">ID</th>
                    <th>
            <%= t("activerecord.attributes.dns_name.name", :default => t("activerecord.labels.name", :default => "Name")) %>
          </th>
                    <th><%= t("web-app-theme.created_at", :default => "Created at")  %></th>
          <th class="last">&nbsp;</th>
        </tr>
        <% @dns_names.each do |dns_name| -%>
        <tr class="<%= cycle("odd", "even") %>">
          <td>
            <%= dns_name.id %>
          </td>
                    <td>
            <%= link_to dns_name.name, dns_name_path(dns_name) %>
          </td>
                    <td>
            <%= dns_name.created_at %>
          </td>
          <td class="last">
            <%= link_to "#{t("web-app-theme.show", :default => "Show")}", dns_name_path(dns_name) %> |
            <%= link_to "#{t("web-app-theme.edit", :default => "Edit")}", edit_dns_name_path(dns_name) %> |
            <%= link_to "#{t("web-app-theme.delete", :default => "Delete")}", dns_name_path(dns_name), :method => :delete, :confirm => "#{t("web-app-theme.confirm", :default => "Are you sure?")}" %>
          </td>
        </tr>          
        <% end -%>
      </table>
      <div class="actions-bar wat-cf">
        <div class="actions">
        </div>
        
      </div>
    </div>
  </div>
</div>

<% content_for :sidebar, render(:partial => 'sidebar') -%>
