<div class="row">
  <div class="span4">
    {{input value=searchText name='Search' placeholder='Search'}}
    <div>
      <a href='/items.csv'>Export Items to CSV</a>
    </div>
    {{
      table-component
      hasFooter=false
      columnsBinding="columns"
      contentBinding="content"
      selection=chosen
    }}

    <a href="/items/new" class="btn btn-primary">New</a>
  </div>
  <div class="span6">
    {{outlet}}
  </div>
</div>
